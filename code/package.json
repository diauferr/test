{
  "name": "plat-react-ui",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "HTTPS=false PORT=3000 HOST=0.0.0.0 react-scripts start",
    "start-staging": "REACT_APP_ENVIRONMENT=staging react-scripts start",
    "build-prod-windows": "REACT_APP_ENVIRONMENT=production && react-scripts build",
    "build": "REACT_APP_ENVIRONMENT=production env-cmd -f .env.production react-scripts build",
    "build:staging": "REACT_APP_ENVIRONMENT=staging env-cmd -f .env.staging react-scripts build",
    "build-staging-windows": "SET REACT_APP_ENVIRONMENT=staging&&react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "find-deadcode": "ts-prune",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint ./src/**/*.tsx",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --fix",
    "lint:js": "npm run lint:eslint -- ./src",
    "lint:js:fix": "eslint --fix ./src",
    "lint:js:format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "lint:staged": "lint-staged"
  },
  "dependencies": {
    "@ant-design/icons": "^2.1.1",
    "@sentry/browser": "^5.11.1",
    "antd": "^3.26.7",
    "auth0-js": "^9.20.1",
    "axios": "^0.21.1",
    "date-fns": "^2.29.3",
    "highlight-words-core": "^1.2.2",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-autosuggest": "^9.4.3",
    "react-datepicker": "^2.11.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-horizontal-scrolling": "^0.1.13",
    "react-horizontal-scrolling-menu": "^3.2.3",
    "react-image-lightbox": "^5.1.1",
    "react-input-mask": "^2.0.4",
    "react-progressive-image": "^0.6.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-scripts": "2.1.8",
    "rxjs": "^6.5.4",
    "styled-components": "^4.4.1",
    "typescript": "^4.9.4",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/auth0-js": "^9.14.7",
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/faker": "^5.5.8",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.191",
    "@types/moment": "^2.13.0",
    "@types/node": "11.12.2",
    "@types/react": "16.8.10",
    "@types/react-autosuggest": "^9.3.13",
    "@types/react-dom": "16.8.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "4.0.3",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "axios-mock-adapter": "^1.17.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "faker": "^5.5.3",
    "firebase": "^8.9.1",
    "firebase-tools": "^9.16.2",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "json-server": "^0.17.1",
    "lint-staged": "^13.1.0",
    "msw": "^0.34.0",
    "prettier": "^2.8.3",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-prune": "^0.10.2"
  }
}
